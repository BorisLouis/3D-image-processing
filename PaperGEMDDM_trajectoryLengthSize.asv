MainFolder = 'D:\DDM_TestData';
ParticleFolders = {'PS_100nm', 'PS_200nm', 'PS_500nm_highconc', 'PS_1000nm_highconc'};

for i = 1:numel(ParticleFolders)
    Path = fullfile(MainFolder, ParticleFolders{i});
    Folder = dir(Path);
    Sizes = [];
    Number = [];
    for j = 3:size(Folder)
        if Folder(j).isdir == 1
            try
                load(fullfile(Folder(j).folder, Folder(j).name, 'msdRes.mat'));
            catch
                load(fullfile(Folder(j).folder, Folder(j).name, 'msdResD.mat'));
            Sizes = [Sizes; [allRes.num]'];
            Number = [Number; size(allRes, 2)];
        end
    end
    disp(append(ParticleFolders{i}, ': ', num2str(sum(Number)), ' traces of ', num2str(mean(Sizes)), ' length'))
end